<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HenCoderView.conflict.ConflictMainAct">

    <Button
        android:onClick="jump_wrongXY"
        android:id="@+id/btn_xywrong"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="上下冲突"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_xysolveByInner"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="jump_xyByInner"
        android:text="内部拦截方式"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_xysolveByOutter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="jump_xyByOutter"
        android:text="外部拦截"
        app:layout_constraintEnd_toStartOf="@+id/btn_xysolveByInner"
        app:layout_constraintStart_toEndOf="@+id/btn_xywrong"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteY="5dp" />
</androidx.constraintlayout.widget.ConstraintLayout>